Functions.
  R b.
  R B.
  R A.
  R ep.
End.

ProgramVariables.
  R xf.
  R vf.
  R af.
  R xl.
  R vl.
  R al.
  R t.
  R xfpost.
  R vfpost.
  R afpost.
  R xlpost.
  R vlpost.
  R alpost.
  R tpost.
End.

Problem.
     (xf < xl & xf + vf^2/(2*b) < xl + vl^2/(2*B) & B >= b & b > 0 & vf >= 0 & vl >= 0 & A >= 0 & ep > 0)
  -> <
      al :=*;
      ?-B <= al & al <= A;
      {  ?xf + vf^2/(2*b) + (A/b+1)*(A/2*ep^2 + ep*vf) < xl + vl^2/(2*B);
         af :=*;
         ?-B <= af & af <= A;
      ++ ?vf = 0; af := 0;
      ++ af :=*; ?-B <= af & af <= -b;
      };
      t := 0;
     > (xfpost = xf & vfpost = vf & afpost = af &
        xlpost = xl & vlpost = vl & alpost = al &
        tpost = t)
End.